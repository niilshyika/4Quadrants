{
  "manifest_version": 2,
  "name": "4Quadrants",
  "description": "This extension helps to prioritize tasks.",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "history"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}