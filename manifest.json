{
  "manifest_version": 2,
  "name": "4Quadrants",
  "description": "This extension helps to prioritize tasks.",
  "version": "1.0",
  "browser_action": {
    // "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}